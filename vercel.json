{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/stripe.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/stripe/webhook",
      "dest": "api/stripe.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}